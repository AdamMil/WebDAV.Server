<compressionMap xmlns="http://adammil.net/webdav.server/compressionMap"
                xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                xsi:schemaLocation="http://adammil.net/webdav.server/compressionMap Compression.xsd">
  <entry mediaType="text/*" />
  <entry mediaType="application/xml" />
  <entry mediaType="application/*+xml" />
  <entry mediaType="application/*+json" />
  <entry mediaType="application/javascript" />
  <entry mediaType="message/*" />
</compressionMap>
